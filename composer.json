{
    "name": "aboutcoders/job-bundle",
    "type": "symfony-bundle",
    "description": "A symfony bundle for asynchronous job processing.",
    "keywords": ["job", "jobs", "schedule", "scheduler", "cron", "cronjob", "bundle", "queue", "task", "background", "rest", "rest-api" , "asynchronous"],
    "homepage": "http://aboutcoders.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "hannes.schulz@aboutcoders.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "~2.6|~3.0",
        "myclabs/php-enum": "~1.2",
        "gedmo/doctrine-extensions": "~2.0",
        "jms/serializer-bundle": "~1.0",
        "nelmio/api-doc-bundle": "~2",
        "doctrine/annotations": "1.*",
        "aboutcoders/process-control": "~1.2",
        "aboutcoders/scheduler-bundle": "~1.2",
        "aboutcoders/resource-lock-bundle": "dev-master",
        "aboutcoders/enum-serializer-bundle": "~0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "php-mock/php-mock-phpunit": "^1.1",
        "mikey179/vfsStream": "^1.5",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/monolog-bundle": "~2",
        "symfony/swiftmailer-bundle": "~2.3",
        "sonata-project/notification-bundle": "~2.2",
        "yzalis/supervisor-bundle": "~1",
        "zendframework/zendxml": "~1",
        "bernard/bernard-bundle" : "dev-master",
        "aboutcoders/process-control-bundle": "~1.2"
    },
    "suggest": {
        "aboutcoders/process-control-bundle": "Adds process control, requires ^1.2",
        "aboutcoders/resource-lock-bundle": "Adds resource locking for for a setup with multiple job processing nodes, requires dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Abc\\Bundle\\JobBundle\\": "" }
    }
}
